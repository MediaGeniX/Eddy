.card
  .card-block
    %h4.card-title= title
    %h6.card-subtitle.text-muted
      = sub_title
    %p.card-text
      %table.table.table-sm
        %thead.thead-default
          %tr
            %th #
            %th Name
            %th.text-right= table_header
        %tbody
          - data.each_with_index do |value, i|
            %tr{class: ("table-success" if user_signed_in? && current_user.name == user_block[value].name)}
              %th{scope: 'row'}= i+1
              %td= user_block[value].name
              %td.text-right= extract_block[value]
